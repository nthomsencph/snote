generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Entry {
  id          String   @id @default(cuid())
  index       Int      @default(autoincrement())
  title       String
  content     String   @db.Text
  preview     String   @db.Text
  date        DateTime @default(now())
  lastUpdated DateTime?
  icon        String?

  @@map("entries")
}
